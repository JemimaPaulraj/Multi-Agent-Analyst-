# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key

# Bedrock Model
# Options: amazon.nova-pro-v1:0, amazon.nova-lite-v1:0, anthropic.claude-3-sonnet-20240229-v1:0
BEDROCK_MODEL_ID=amazon.nova-pro-v1:0

# Database Configuration
DB_HOST=your-rds-endpoint.amazonaws.com
DB_PORT=3306
DB_USER=your-username
DB_PASSWORD=your-password
DB_NAME=your-database

# S3 Configuration (for RAG documents)
S3_BUCKET=your-bucket-name
S3_PREFIX=RAG_Data/

# SageMaker Forecasting Endpoint
SAGEMAKER_ENDPOINT=your-sagemaker-endpoint

# Bedrock Guardrails (optional)
BEDROCK_GUARDRAIL_ID=your-guardrail-id
BEDROCK_GUARDRAIL_VERSION=DRAFT

# OpenAI Key (optional backup)
OPENAI_API_KEY=your-openai-key
